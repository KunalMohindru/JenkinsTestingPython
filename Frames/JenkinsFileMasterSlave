pipeline {
    agent any

    stages {

        stage('Install dependencies') {
            steps {
                sh """
                pip install -r Frames/requirements.txt
                """
            }
        }

        stage('Run Tests') {
            steps {
                sh """
                pytest -s -v --browser_name=headless Frames/Test/test_google.py
                """
            }
        }
    }
}
